<%- include ('../partials/header')%>
<%- include ('../partials/nav')%>
<div class="container">
<div class="row">
        <%- include ('../partials/sideNav')%>
        <div class="col m12">
         <div class="container center-align">
            <div class="row">
            <div class="col s12">
            <h3>Export data in CSV</h>
                <br>
                <br>
            <div class="row">
             <form class="col s12 center-align" method="POST" action="/add/export">
              <input type="hidden" name="_csrf" value="<%= csrfToken %>">
               <div class="row">
                 <div class="input-field col s6">
                   <input id="initial" name="initial" type="text" class="datepicker" required>
                   <label for="initial">Initial Date</label>
                 </div>
                <div class="input-field col s6">
                    <input id="final" name="final" type="text" class="datepicker" required>
                    <label for="final">Final Date</label>
                </div>
              </div>
               <div class="row right-align">
                <button type="submit" class="btn waves-effect waves-light teal">Export</button>
               </div>
             </form>
           </div>
        </div>
         </div>
        </div>
        </div>
    </div>
</div>
<%- include('../partials/footer') %>